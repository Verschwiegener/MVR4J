<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="GeneralSceneDescription" type="GeneralSceneDescriptionType"/>
  <xs:complexType name="SceneObjectType" mixed="true">
    <xs:sequence>
      <xs:element type="xs:string" name="Matrix" minOccurs="0"/>
      <xs:element type="GeometriesType" name="Geometries" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="optional"/>
    <xs:attribute type="xs:string" name="uuid" use="optional"/>
  </xs:complexType>
  <xs:complexType name="SymbolType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:string" name="symdef" use="optional"/>
        <xs:attribute type="xs:string" name="uuid" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="GeometriesType">
    <xs:sequence>
      <xs:element type="SymbolType" name="Symbol"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressType">
    <xs:simpleContent>
      <xs:extension base="xs:byte">
        <xs:attribute type="xs:byte" name="break" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="AddressesType">
    <xs:sequence>
      <xs:element type="AddressType" name="Address" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FixtureType" mixed="true">
    <xs:sequence>
      <xs:element type="xs:string" name="Matrix" minOccurs="0"/>
      <xs:element type="xs:string" name="GDTFSpec" minOccurs="0"/>
      <xs:element type="xs:string" name="GDTFMode" minOccurs="0"/>
      <xs:element type="AddressesType" name="Addresses" minOccurs="0"/>
      <xs:element type="xs:byte" name="FixtureID" minOccurs="0"/>
      <xs:element type="xs:byte" name="UnitNumber" minOccurs="0"/>
      <xs:element type="xs:byte" name="FixtureTypeId" minOccurs="0"/>
      <xs:element type="xs:byte" name="CustomId" minOccurs="0"/>
      <xs:element type="xs:string" name="Color" minOccurs="0"/>
      <xs:element type="xs:string" name="CastShadow" minOccurs="0"/>
      <xs:element type="xs:string" name="Mappings" minOccurs="0"/>
      <xs:element type="GeometriesType" name="Geometries" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="optional"/>
    <xs:attribute type="xs:string" name="uuid" use="optional"/>
  </xs:complexType>
  <xs:complexType name="ChildListType" mixed="true">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
      <xs:element type="SceneObjectType" name="SceneObject"/>
      <xs:element type="FixtureType" name="Fixture"/>
      <xs:element type="Geometry3DType" name="Geometry3D"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="LayerType">
    <xs:sequence>
      <xs:element type="ChildListType" name="ChildList"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name"/>
    <xs:attribute type="xs:string" name="uuid"/>
  </xs:complexType>
  <xs:complexType name="LayersType">
    <xs:sequence>
      <xs:element type="LayerType" name="Layer"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ClassType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:string" name="name" use="optional"/>
        <xs:attribute type="xs:string" name="uuid" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="Geometry3DType" mixed="true">
    <xs:sequence>
      <xs:element type="xs:string" name="Matrix" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="fileName" use="optional"/>
  </xs:complexType>
  <xs:complexType name="SymdefType">
    <xs:sequence>
      <xs:element type="ChildListType" name="ChildList"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="optional"/>
    <xs:attribute type="xs:string" name="uuid" use="optional"/>
  </xs:complexType>
  <xs:complexType name="AUXDataType">
    <xs:sequence>
      <xs:element type="ClassType" name="Class" maxOccurs="unbounded" minOccurs="0"/>
      <xs:element type="SymdefType" name="Symdef" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SceneType">
    <xs:sequence>
      <xs:element type="LayersType" name="Layers"/>
      <xs:element type="AUXDataType" name="AUXData"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GeneralSceneDescriptionType">
    <xs:sequence>
      <xs:element type="xs:string" name="UserData"/>
      <xs:element type="SceneType" name="Scene"/>
    </xs:sequence>
    <xs:attribute type="xs:byte" name="verMajor"/>
    <xs:attribute type="xs:byte" name="verMinor"/>
  </xs:complexType>
</xs:schema>